{
   "compilerOptions": {
      "module": "esnext",
      "target": "esnext",
      "moduleResolution": "node",
      "allowJs": true,
      "noEmit": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "baseUrl": "src",
      "jsx": "react",
      "paths": {
         "_/*": ["*"]
      }
   },
   "include": ["src"]
}
